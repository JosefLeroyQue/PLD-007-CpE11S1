#include <iostream>
using namespace std;

int main() {
    // Initialize the array
    int array[10] = {19, 3, 15, 7, 11, 9, 13, 5, 17, 1};

    // Display the header
    cout << "Element    Value    Histogram" << endl;

    // Loop through the array and print the desired output
    for (int index = 0; index < 10; index++) {
        cout << index << "          " << array[index] << "    ";
        for (int j = 0; j < array[index]; j++) {
            cout << "*";
        }
        cout << endl;
    }

    return 0;
}

#include <iostream>
using namespace std;

int main() {
    // Initialize the array of responses
    int responses[40] = {1, 2, 6, 4, 8, 5, 9, 7, 8, 10,
                         1, 6, 3, 8, 6, 10, 3, 8, 2, 7,
                         6, 5, 7, 6, 8, 6, 7, 5, 6, 6,
                         5, 6, 7, 5, 6, 4, 8, 6, 8, 10};

    // Initialize a count array for responses from 1 to 10
    int counts[11] = {0}; // Index 0 will be unused

    // Count the occurrences of each response
    for (int i = 0; i < 40; i++) {
        if (responses[i] >= 1 && responses[i] <= 10) {
            counts[responses[i]]++;
        }
    }

    // Display the summary
    cout << "Response    Count" << endl;
    for (int i = 1; i <= 10; i++) {
        cout << i << "          " << counts[i] << endl;
    }

    return 0;
}
